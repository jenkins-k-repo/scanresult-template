{
	"plugins": [],
	"settings": {},
	"env": {
		"es6": true,
		"mocha": true,
		"node": true
	},
	"extends": [
		"eslint:recommended"
	],
	"parserOptions": {},
	"rules": {
		"no-console": 0,
		"indent": [
			"error",
			"tab",
			{
				"SwitchCase": 1
			}
		],
		"linebreak-style": [
			"error",
			"unix"
		],
		"quotes": [
			"error",
			"single"
		],
		"semi": [
			"error",
			"never"
		]
	},
	"globals": {}
}